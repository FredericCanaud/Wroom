   {{/each}}
   <tr>
     <td></td>
     <form action="" method="post">
       <div>
           <input type="hidden" name="gpnum" value="{{gpnum}}">
       </div>
       <td>{{nombrePilotesNotInGP}}</td>
       <td>
         <div>
           <select name="pilnum">
           {{#each pilotesNotInGP}}
              <option value="{{this.pilnum}}">{{this.pilnom}}</option>
           {{/each}}
           </select>
         </div>
       </td>
       <td>
         <input type="number" id="timeTrial" name="minutes" min="00" max="59" value="00"> : <input type="number" name="secondes" id="timeTrial" min="00" max="59" value="00"> : <input type="number" id="timeTrial" name="millisecondes" min="00" max="59" value="00">
