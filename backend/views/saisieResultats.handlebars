<div class="titre">
  <img src="/image/cat_resultat.png">
  <h1> Saisie des r√©sultats </h1>
</div>
<table border="1">
		<tr>
      <th> </th>
			<th> Place </th>
			<th> Pilote </th>
			<th> Temps </th>
			<th> Nb points </th>
		</tr>
	 {{#each resultat}}
		<tr>
      <td> <img src="/image/act_delete.png"> </td>
