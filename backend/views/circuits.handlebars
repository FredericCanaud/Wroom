<h1> Gestion des Circuits </h1>
<img src="/image/cat_circuit.png">
<div>
  <img src="/image/act_add.png">
  <a href="/ajouterCircuit">Ajouter un circuit</a>
</div>
<table border="1">
		<tr>
			<th> Nom </th>
			<th> Longueur </th>
			<th> Nombre de spectateurs </th>
			<th> Actions </th>
		</tr>
	 {{#each circuits}}
		<tr>
			<td> {{this.cirnom}} </td>
      <td> {{this.cirlongueur}} </td>
			<td> {{this.cirnbspectateurs}} </td>
			<td>
          <img src="/image/act_edit.png"> <a href="modifierCircuit\:num">Modifier</a>
          <img src="/image/act_delete.png"> <a href="supprimerCircuit\:num">Supprimer</a>
      </td>
    </tr>
   {{/each}}
</table>
